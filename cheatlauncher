local function createNotify(text)
	local notif = Notify:Clone()
	notif.Parent = notifies
	notif.Text = text
	notif.Visible = true
	game.Debris:AddItem(notif, math.random(3,5))
end
local posX = 0
local posY = 0
local xPlus = 0.1
local function createWindow()
	local ids = {"135413396810531", "51657274", "13979677676", "132408240384029", "8990736976", "7255938903", "2972135333", "123574199714471", "101376476918347", "10657365540", "83046327", "15296361385"}
	local id = ids[math.random(1, #ids)]
	local win = viru:Clone()
	win.Image = "rbxassetid://"..id
	win.Parent = MAIN__88192
	win.Visible = true
	win.Name = "MAIN"..math.random(1, 100000)
	posY += 0.03
	if posY >= 1 then
		posY = 0
	end
	posX += 0.01
	if posX >= 1 then
		posX = xPlus
		xPlus += 0.1
	end
	win.Position = UDim2.new(posX, 0, posY, 0)
end

